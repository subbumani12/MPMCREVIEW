{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python",
      "config": { "runtime": "python3.9" }
    }
  ],
  "routes": [
    { "src": "/.*", "dest": "app.py" }
  ],
  "env": {
    "PYTHONUNBUFFERED": "1"
  },
  "includeFiles": [
    "face_recognizer.yml",
    "label_map.npy",
    "face_data/*"
  ]
}
